<div class="container mt-4">
  <div class="row">
    <div class="col-md-8">
      <article class="speakerslist maincontent">
        <h1 class="speakerslist-title"><%=member.title%></h1>
        <div class="speakerslist-name">
          with
          <a href="/members/<%=member.shortname%>"><%=member.name%></a>
        </div>
        <p class="speakerslist-info mt-2">
          <a href="/members/<%=member.shortname%>"> </a>
          <img
            class="speakerslist-img img-fluid"
            src="/images/members/<%=member.shortname%>.jpeg"
            alt="Photo of <%=member.name%>"
          />
        </p>
        <%-member.description%>
        <p></p>
      </article>
    </div>
    <aside class="col-md-4">
      <%- include('./partials/artwork')%>
    </aside>
  </div>
</div>
